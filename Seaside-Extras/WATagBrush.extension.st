Extension { #name : 'WATagBrush' }

{ #category : '*Bootstrap5/Core' }
WATagBrush >> textCenter [

	self class: 'text-center'
]

{ #category : '*Bootstrap5/Core' }
WATagBrush >> textEnd [

	self class: 'text-end'
]

{ #category : '*Bootstrap5/Core' }
WATagBrush >> textStart [

	self class: 'text-start'
]
